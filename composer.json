{
    "name": "swayok/peskyorm-laravel",
    "type": "library",
    "description": "Service Providers and other classes to replace Laravel Eloquent ORM by PeskyORM",
    "keywords": [
        "php",
        "laravel",
        "service provider"
    ],
    "authors": [
        {
            "name": "Alexander Filippov"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/swayok/peskyorm-laravel/issues"
    },
    "autoload": {
        "psr-4": {
            "PeskyORMLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeskyORMLaravel\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/framework": ">=5.3",
        "swayok/peskyorm": "v3.x-dev",
        "swayok/peskyorm-columns-collection": "dev-orm_v3"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "orchestra/testbench": "*"
    }
}
