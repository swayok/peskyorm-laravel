{
    "name": "swayok/peskyorm-laravel",
    "type": "library",
    "description": "Service Providers and other classes to replace laravel's Eloquent ORM by PeskyORM",
    "keywords": [
        "php",
        "laravel",
        "service provider"
    ],
    "authors": [
        {
            "name": "Alexander Filippov"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/swayok/peskyorm-laravel/issues"
    },
    "autoload": {
        "psr-4": {
            "PeskyORMLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeskyORMLaravel\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/framework": ">=5.3",
        "swayok/peskyorm": "dev-v2_1"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "orchestra/testbench": "*"
    }
}
